        <common.version>2.2022.09.15_07.46-e4fa96eb6813</common.version>
        <modia-common-utils.version>1.2022.11.08-10.35-554219ed136d</modia-common-utils.version>
        <tjenestespesifikasjoner.version>2606.1f6354e</tjenestespesifikasjoner.version>
        <org.jvnet.jaxb2_commons.version>0.12.0</org.jvnet.jaxb2_commons.version>
    </properties>

    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>no.nav.common</groupId>
                <artifactId>nais</artifactId>
                <version>${common.version}</version>
            </dependency>
            <dependency>
                <groupId>no.nav.common</groupId>
                <artifactId>rest</artifactId>
                <version>${common.version}</version>
            </dependency>
            <dependency>
