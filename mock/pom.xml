<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <parent>
        <groupId>no.nav.sbl.dialogarena</groupId>
        <artifactId>modiabrukerdialog</artifactId>
        <version>2.0.0-SNAPSHOT</version>
    </parent>

    <artifactId>modiabrukerdialog-mock</artifactId>
    <name>${project.artifactId}</name>

    <dependencies>
        <dependency>
            <groupId>no.nav</groupId>
            <artifactId>kjerneinfo-consumer</artifactId>
        </dependency>

        <dependency>
            <groupId>no.nav</groupId>
            <artifactId>sykmeldingsperioder-panel</artifactId>
        </dependency>

        <dependency>
            <groupId>no.nav</groupId>
            <artifactId>sykmeldingsperioder-consumer</artifactId>
        </dependency>

        <dependency>
            <groupId>no.nav</groupId>
            <artifactId>kodeverk-consumer</artifactId>
        </dependency>
        <dependency>
            <groupId>no.nav.sbl.dialogarena</groupId>
            <artifactId>common-kodeverk</artifactId>
        </dependency>

        <dependency>
            <groupId>no.nav.sbl.dialogarena</groupId>
            <artifactId>kodeverk-tjenestespesifikasjon</artifactId>
        </dependency>

        <dependency>
            <groupId>no.nav.sbl.dialogarena</groupId>
            <artifactId>sakogbehandling-tjenestespesifikasjon</artifactId>
        </dependency>

        <dependency>
            <groupId>no.nav.sbl.dialogarena</groupId>
            <artifactId>utbetaling-tjenestespesifikasjon</artifactId>
        </dependency>

        <dependency>
            <groupId>no.nav.tjenester.fim</groupId>
            <artifactId>nav-fim-aktoer-v1-tjenestespesifikasjon</artifactId>
            <classifier>jaxws</classifier>
        </dependency>

        <dependency>
            <groupId>no.nav.sbl.dialogarena</groupId>
            <artifactId>soknader-liste</artifactId>
        </dependency>

        <dependency>
            <groupId>no.nav</groupId>
            <artifactId>brukerprofil-consumer</artifactId>
        </dependency>

        <dependency>
            <groupId>no.nav</groupId>
            <artifactId>behandlebrukerprofil-consumer</artifactId>
        </dependency>

        <dependency>
            <groupId>no.nav.modig</groupId>
            <artifactId>modig-wicket</artifactId>
        </dependency>

        <dependency>
            <groupId>no.nav</groupId>
            <artifactId>kjerneinfo-domain</artifactId>
        </dependency>

        <dependency>
            <groupId>org.springframework</groupId>
            <artifactId>spring-context</artifactId>
        </dependency>

        <dependency>
            <groupId>org.mockito</groupId>
            <artifactId>mockito-core</artifactId>
        </dependency>

        <dependency>
            <groupId>no.nav</groupId>
            <artifactId>brukerprofil-domain</artifactId>
        </dependency>

        <dependency>
            <groupId>joda-time</groupId>
            <artifactId>joda-time</artifactId>
        </dependency>

        <dependency>
            <groupId>no.nav</groupId>
            <artifactId>kjerneinfo-common</artifactId>
        </dependency>

    </dependencies>

    <build>
        <plugins>
            <plugin>
                <artifactId>maven-dependency-plugin</artifactId>
                <executions>
                    <execution>
                        <id>analyze</id>
                        <goals>
                            <goal>analyze-only</goal>
                        </goals>
                        <configuration>
                            <usedDependencies>
                                <usedDependency>no.nav.sbl.dialogarena:soknader-liste</usedDependency>
                            </usedDependencies>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>

</project>