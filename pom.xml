                <artifactId>pleiepenger-v1-tjenestespesifikasjon</artifactId>
                <version>${tjenestespesifikasjoner.version}</version>
            </dependency>

            <!--Modia common-->
            <dependency>
                <groupId>com.github.navikt.modia-common-utils</groupId>
                <artifactId>kotlin-utils</artifactId>
                <version>${modia-common-utils.version}</version>
            </dependency>
            <dependency>
                <groupId>com.github.navikt.modia-common-utils</groupId>
                <artifactId>test-utils</artifactId>
                <version>${modia-common-utils.version}</version>
                <scope>test</scope>
            </dependency>
            <dependency>
                <groupId>com.github.navikt.modia-common-utils</groupId>
                <artifactId>typeanalyzer</artifactId>
                <version>${modia-common-utils.version}</version>
            </dependency>
            <dependency>
                <groupId>com.github.navikt.modia-common-utils</groupId>
