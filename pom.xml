        <commons-lang3.version>3.3.2</commons-lang3.version>
        <hamcrest.version>1.3</hamcrest.version>
        <cxf.version>2.7.7</cxf.version>
        <ehcache.version>2.6.10</ehcache.version>
        <slf4j.version>1.7.5</slf4j.version>
        <collections-generic.version>4.01</collections-generic.version>

        <!-- Egenlagde -->
        <common-jetty.version>1.0.23</common-jetty.version>
        <common-test.version>1.2</common-test.version>
        <common-time.version>3.1.3</common-time.version>
        <modia-felleskomponenter.version>3.2</modia-felleskomponenter.version>
        <modig.frontend.version>1.1.108</modig.frontend.version>
        <modig-security.version>1.0.24-NO_RESTEASY</modig-security.version>
        <utbetaling.tjenestespesifikasjon.version>1.0.15</utbetaling.tjenestespesifikasjon.version>

        <!-- Test -->
        <mockito.version>1.9.5</mockito.version>
