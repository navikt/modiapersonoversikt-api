<?xml version="1.0" encoding="UTF-8"?>
<application xmlns="http://appconfig.aura.nav.no" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://appconfig.aura.nav.no http://maven.adeo.no/xsd/appconfig.xsd">

    <name>modiabrukerdialog</name>

    <selftest>
        <path>/modiabrukerdialog/internal/selftest</path>
        <humanReadablePath>/modiabrukerdialog/internal/selftest</humanReadablePath>
    </selftest>

    <artifacts>
        <ear groupId="no.nav.sbl.dialogarena" artifactId="modiabrukerdialog-jee"/>
    </artifacts>

    <security serviceUserResourceAlias="srvModiabrukerdialog">
        <logins>
            <openIdConnect moduleClass="no.nav.aura.loginModule.JWTLoginModule">
                <contextRoot>/modiabrukerdialog/rest/login</contextRoot>
            </openIdConnect>
        </logins>
    </security>

    <loadBalancer isAlive="/modiabrukerdialog/internal/isAlive" />
    <logging serviceCalls="true" />

    <exposed-services>
        <exposedRest path="/modiabrukerdialog/rest" name="personoversiktAPI" />
    </exposed-services>

    <resources>
        <!-- Felles -->
        <webservice alias="virksomhet:Personsok_v1" mapToProperty="personsokendpoint"/>
        <ldap alias="ldap" mapToProperty="ldap"/>
        <!-- Slutt felles -->
        <baseUrl alias="tjenestebuss" mapToProperty="tjenestebuss"/>
        <baseUrl alias="tjenester" mapToProperty="tjenester"/>
        <credential alias="srv_kjerneinfo_tjenestebuss" mapToProperty="ctjenestebuss"/>
        <directory name="modiabrukerdialog" mapToProperty="modiabrukerdialog.datadir"/>
        <baseUrl alias="server.gosys" mapToProperty="server.gosys"/>
        <baseUrl alias="server.pesys" mapToProperty="server.pesys"/>
        <baseUrl alias="server.arena" mapToProperty="server.arena"/>
        <baseUrl alias="server.aareg" mapToProperty="server.aareg"/>
        <baseUrl alias="server.drek" mapToProperty="server.drek"/>
        <baseUrl alias="server.aktivitetsplan" mapToProperty="server.aktivitetsplan"/>
        <baseUrl alias="server.veilarbportefoljeflatefs" mapToProperty="server.veilarbportefoljeflatefs"/>
        <baseUrl alias="server.norg2-frontend" mapToProperty="server.norg2-frontend"/>
        <webservice alias="Aktoer_v1" mapToProperty="aktorid.ws"/>
        <webservice alias="virksomhet:TildelOppgave_v1" mapToProperty="gsak.tildeloppgave.v1"/>
        <webservice alias="virksomhet:Oppgavebehandling_v3" mapToProperty="gsak.oppgavebehandling.v3"/>
        <webservice alias="virksomhet:Oppgave_v3" mapToProperty="gsak.oppgave.v3"/>
        <webservice alias="virksomhet:Sak_v1" mapToProperty="gsak.sak.v1"/>
        <webservice alias="virksomhet:BehandleSak_v1" mapToProperty="gsak.behandlesak.v1"/>
        <webservice alias="virksomhet:ArbeidOgAktivitet_v1" mapToProperty="arena.arbeidogaktivitet.v1"/>
        <webservice alias="virksomhet:Person_v3" mapToProperty="tps.person.v3"/>
        <webservice alias="virksomhet:BehandlePerson_v1" mapToProperty="tps.behandleperson.v1"/>
        <webservice alias="virksomhet:Organisasjon_v4" mapToProperty="organisasjonendpoint.v4"/>
        <webservice alias="virksomhet:Kodeverk_v2" mapToProperty="kodeverkendpoint.v2"/>
        <webservice alias="virksomhet:BehandleBrukerprofil_v2" mapToProperty="behandlebrukerprofilendpoint"/>
        <webservice alias="virksomhet:Ytelseskontrakt_v3" mapToProperty="ytelseskontraktendpoint"/>
        <webservice alias="virksomhet:Oppfolging_v1" mapToProperty="oppfolgingendpoint"/>
        <webservice alias="OppfolgingsinfoV2" mapToProperty="oppfolgingsinfo.v1"/>
        <webservice alias="virksomhet:foreldrepenger_v2" mapToProperty="foreldrepengerendpoint"/>
        <webservice alias="virksomhet:pleiepenger_v1" mapToProperty="pleiepengerendpoint"/>
        <webservice alias="virksomhet:sykepenger_v2" mapToProperty="sykepengerendpoint"/>
        <webservice alias="virksomhet:DigitalKontakinformasjon_v1" mapToProperty="dkifendpoint"/>
        <webservice alias="virksomhet:OrganisasjonEnhet_v2" mapToProperty="norg2.organisasjonenhet.v2"/>
        <webservice alias="virksomhet:OrganisasjonEnhetKontaktinformasjon_v1" mapToProperty="norg2.organisasjonenhetkontaktinformasjon.v1"/>
        <webservice alias="virksomhet:EgenAnsatt_V1" mapToProperty="egenansatt.v1" />
        <webservice alias="virksomhet:Arbeidsfordeling_v1" mapToProperty="arbeidsfordeling.v1" />
        <webservice alias="Utbetaling_v1" mapToProperty="utbetalingendpoint"/>
        <webservice alias="domene.Brukerdialog:Henvendelse_v2" mapToProperty="henvendelse.v2"/>
        <webservice alias="domene.Brukerdialog:SendUtHenvendelse_v1" mapToProperty="send.ut.henvendelse"/>
        <webservice alias="domene.Brukerdialog:HenvendelseSoknaderService_v1" mapToProperty="henvendelser.ws"/>
        <webservice alias="domene.Brukerdialog:BehandleHenvendelse_v1" mapToProperty="behandle.henvendelse"/>
        <webservice alias="BrukervarselV1" mapToProperty="varsler.ws"/>
        <webservice alias="SakOgBehandling" mapToProperty="sakogbehandling.ws"/>
        <webservice alias="pensjon:PensjonSak_v1" mapToProperty="pensjon.sak.v1"/>
        <webservice alias="Journal_v2" mapToProperty="journal.v2"/>
        <webservice alias="InnsynJournal_v2" mapToProperty="innsyn.journal.v2"/>
        <rest alias="kodeverksmapper.oppgavetype" mapToProperty="kodeverksmapper.oppgavetype"/>
        <rest alias="kodeverksmapper.underkategori" mapToProperty="kodeverksmapper.underkategori"/>
        <rest alias="kodeverksmapper.ping" mapToProperty="kodeverksmapper.ping"/>
        <rest alias="unleash-api" mapToProperty="unleash"/>
        <applicationProperties alias="modiabrukerdialog.properties"/>
        <baseUrl alias="modiabrukerdialog.standardtekster.tilbakemelding" mapToProperty="modiabrukerdialog.standardtekster.tilbakemelding"/>
        <!-- App-->
        <baseUrl alias="appres.cms" mapToProperty="appres.cms"/>

        <applicationCertificate alias="srvModiabrukerdialog"/>

        <baseUrl alias="isso-host"/>
        <baseUrl alias="isso-jwks"/>
        <baseUrl alias="isso-issuer"/>
        <baseUrl alias="isso.isalive"/>
        <credential alias="isso-rp-user"/>
    </resources>

    <serverOptions>
        <memoryParameters resourceAlias="modiabrukerdialog-memparams"/>
        <jvmArgs>-XX:+UseG1GC</jvmArgs>
    </serverOptions>

</application>
