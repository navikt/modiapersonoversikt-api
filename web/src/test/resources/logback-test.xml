<?xml version="1.0" encoding="UTF-8"?>
<configuration scan="true" scanPeriod="10 seconds">
    <property name="MAIN_APPLICATION" value="modiabrukerdialog"/>

    <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%date %level [%thread] - [%logger] - %m%n</pattern>
        </encoder>
    </appender>

    <logger name="no.nav.modig" level="WARN"/>
    <logger name="org.apache.wicket" level="WARN"/>
    <logger name="org.springframework" level="WARN"/>
    <logger name="no.nav.modig.content.ValuesFromContentWithResourceBundleFallback" level="ERROR"/>

    <!--kaster veldig mange exceptions, ligger under no.nav.sbl.dialogarena.common.cxf-->
    <logger name="no.nav.sbl.dialogarena.common.cxf.InstanceSwitcher" level="WARN" />

    <!--fjerner sope kall-->
    <logger name="org.apache.cxf" level="WARN" />
    <!--fjerner sope kall med innhold-->
    <logger name="no.nav.sbl.dialogarena.common.cxf" level="WARN" />

    <!--fjern denne hvsi du feilsÃ¸ker problemer med tilgang-->
    <logger name="org.jboss.security.xacml.sunxacml.finder.AttributeFinder" level="WARN" />

    <!--denne fjener warnings om mulig feil lokalisering av tekst-->
    <logger name="org.apache.wicket.Localizer" level="ERROR" />

    <!--viser alle netverkskall fra klent-->
    <logger name="org.apache.wicket.protocol.http.RequestLogger" level="INFO" />

    <root level="INFO">
        <appender-ref ref="CONSOLE"/>
    </root>

</configuration>