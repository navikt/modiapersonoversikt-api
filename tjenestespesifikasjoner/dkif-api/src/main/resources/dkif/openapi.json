{
  "swagger": "2.0",
  "info": {
    "description": "REST-grensesnittet som er tilgjengelig for konsumering av andre applikasjoner og komponenter, for å hente personers digitale kontaktinformasjon.",
    "title": "API versjon 1"
  },
  "host": "dkif.dev.adeo.no",
  "basePath": "/",
  "tags": [
    {
      "name": "Digital kontaktinformasjon",
      "description": "Endepunkt for digital kontaktinformasjon"
    },
    {
      "name": "Ping",
      "description": "Endepunkt for pinging av applikasjonen"
    }
  ],
  "paths": {
    "/api/ping": {
      "get": {
        "tags": [
          "Ping"
        ],
        "summary": "Kalles for å verifisere at applikasjonen svarer",
        "operationId": "getPingUsingGET",
        "produces": [
          "*/*"
        ],
        "parameters": [
          {
            "name": "Nav-Consumer-Id",
            "in": "header",
            "description": "Navnet på servicebrukeren til applikasjonen som gjør kallet.",
            "required": true,
            "type": "string",
            "x-example": "srvmedl2"
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/GetPingResponse"
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          }
        },
        "deprecated": false
      }
    },
    "/api/v1/personer/kontaktinformasjon": {
      "get": {
        "tags": [
          "Digital kontaktinformasjon"
        ],
        "summary": "digitalKontaktinformasjon",
        "operationId": "digitalKontaktinformasjonUsingGET",
        "produces": [
          "*/*"
        ],
        "parameters": [
          {
            "name": "Authorization",
            "in": "header",
            "description": "OIDC-token for autorisering av bruker som gjør kallet.",
            "required": true,
            "type": "string"
          },
          {
            "name": "Nav-Call-Id",
            "in": "header",
            "description": "En identifikator for denne kallrekken som kan brukes i debugging dersom en feil skulle oppstå.",
            "required": true,
            "type": "string"
          },
          {
            "name": "Nav-Consumer-Id",
            "in": "header",
            "description": "Navnet på servicebrukeren til applikasjonen som gjør kallet - for eksempel \"srvmedl2\".",
            "required": true,
            "type": "string"
          },
          {
            "name": "Nav-Personidenter",
            "in": "header",
            "description": "Nav-Personidenter",
            "required": true,
            "type": "array",
            "items": {
              "type": "string"
            },
            "collectionFormat": "multi"
          },
          {
            "name": "inkluderSikkerDigitalPost",
            "in": "query",
            "description": "inkluderSikkerDigitalPost",
            "required": false,
            "type": "boolean",
            "default": false
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/DigitalKontaktinfoBolk"
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          }
        },
        "deprecated": false
      }
    },
    "/api/v1/printsertifikat": {
      "get": {
        "tags": [
          "Digital kontaktinformasjon"
        ],
        "summary": "printsertifikat",
        "operationId": "printsertifikatUsingGET",
        "produces": [
          "*/*"
        ],
        "parameters": [
          {
            "name": "Authorization",
            "in": "header",
            "description": "OIDC-token for autorisering av bruker som gjør kallet.",
            "required": true,
            "type": "string"
          },
          {
            "name": "Nav-Call-Id",
            "in": "header",
            "description": "En identifikator for denne kallrekken som kan brukes i debugging dersom en feil skulle oppstå.",
            "required": true,
            "type": "string"
          },
          {
            "name": "Nav-Consumer-Id",
            "in": "header",
            "description": "Navnet på servicebrukeren til applikasjonen som gjør kallet - for eksempel \"srvmedl2\".",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/Printsertifikat"
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          }
        },
        "deprecated": false
      }
    }
  },
  "definitions": {
    "DigitalKontaktinfo": {
      "type": "object",
      "required": [
        "kanVarsles",
        "reservert"
      ],
      "properties": {
        "epostadresse": {
          "type": "string"
        },
        "kanVarsles": {
          "type": "boolean",
          "description": "Hvorvidt brukeren ikke har reservert seg for digital post, og han/hun kan varsles digitalt i henhold til eForvaltningsforskriftens §32"
        },
        "mobiltelefonnummer": {
          "type": "string"
        },
        "personident": {
          "type": "string"
        },
        "reservert": {
          "type": "boolean",
          "description": "Hvorvidt brukeren har reservert seg for digital post"
        },
        "sikkerDigitalPostkasse": {
          "$ref": "#/definitions/SikkerDigitalPostkasse"
        },
        "spraak": {
          "type": "string",
          "example": "nb",
          "description": "Språket brukeren ønsker å benytte for kommunikasjon. Verdien er en kode fra et lite subsett ISO 639-1 språkkoder."
        }
      },
      "title": "DigitalKontaktinfo"
    },
    "DigitalKontaktinfoBolk": {
      "type": "object",
      "properties": {
        "feil": {
          "type": "object",
          "additionalProperties": {
            "$ref": "#/definitions/Feil"
          }
        },
        "kontaktinfo": {
          "type": "object",
          "additionalProperties": {
            "$ref": "#/definitions/DigitalKontaktinfo"
          }
        }
      },
      "title": "DigitalKontaktinfoBolk"
    },
    "Feil": {
      "type": "object",
      "properties": {
        "melding": {
          "type": "string"
        }
      },
      "title": "Feil"
    },
    "GetPingResponse": {
      "type": "object",
      "properties": {
        "response": {
          "type": "string"
        }
      },
      "title": "GetPingResponse"
    },
    "Printsertifikat": {
      "type": "object",
      "properties": {
        "postkasseleverandoerAdresse": {
          "type": "string"
        },
        "x509Sertifikat": {
          "type": "string"
        }
      },
      "title": "Printsertifikat"
    },
    "SikkerDigitalPostkasse": {
      "type": "object",
      "properties": {
        "adresse": {
          "type": "string"
        },
        "leverandoerAdresse": {
          "type": "string"
        },
        "leverandoerSertifikat": {
          "type": "string"
        }
      },
      "title": "SikkerDigitalPostkasse"
    }
  }
