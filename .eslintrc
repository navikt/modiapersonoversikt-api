{
  "parser": "babel-eslint",
  "rules": {
    "indent": [2, 4, {"SwitchCase": 1}],
    "comma-dangle": [2, "never"],
    "max-len": [2, 120, 4],
    "no-unused-vars": [2, {"varsIgnorePattern": "_", "argsIgnorePattern": "_"}]
  },
  "globals": {
    "$": false,
    "Wicket": false
  },
  "extends": "airbnb"
}